<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefWidth="600.0" spacing="8.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="headerLabel" styleClass="dialog-header-text" text="Are you sure?">
         <VBox.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </VBox.margin>
      </Label>
      <Separator prefWidth="200.0" />
      <Label fx:id="contentLabel" styleClass="dialog-content-text" text="Process with this action?" wrapText="true">
         <VBox.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </VBox.margin>
      </Label>
      <HBox fx:id="buttonGroup" alignment="CENTER_RIGHT" prefWidth="200.0" spacing="8.0" styleClass="dialog-button-group">
         <children>
            <Button fx:id="btnOK" defaultButton="true" mnemonicParsing="false" prefHeight="32.0" prefWidth="100.0" text="Proceed" />
            <Button fx:id="btnCancel" cancelButton="true" mnemonicParsing="false" prefHeight="32.0" prefWidth="100.0" text="Cancel" />
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </padding>
      </HBox>
   </children>
</VBox>
